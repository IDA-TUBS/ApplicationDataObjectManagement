# - Config file for the rscmng library

@PACKAGE_INIT@
include( "${CMAKE_CURRENT_LIST_DIR}/adomTargets.cmake" )

# Check if the package has been loaded already
if(TARGET adom)
  return()
endif()

message(STATUS "Found ADOM ${CMAKE_CURRENT_LIST_DIR}")

find_package(Boost REQUIRED COMPONENTS system)

message(STATUS "Boost was found")

set(ADOM_LIB adom)
set(ADOM_COMPILE_OPTIONS -g)

message(STATUS "ADOM options set ${ADOM_COMPILE_OPTIONS}")